doctype html
head 
	title hitu outdoor
	meta(http-equiv="Content-Type",content="text/html; charset=utf-8")
	link(type="text/css",rel="stylesheet",href="/style/g.css")
	script(src="/script/jquery-3.1.1.js",type="text/javascript")
	script(src="/script/velocity.js",type="text/javascript")
	script(src="/script/index.js",type="text/javascript")
body
	.header
		.header-wrapper
			#header-left.fleft(style="width:58%;")
				a.logo.col25.fleft.clear-a(href="javascript:void(0)" title="hitu logo")
					img#logo(src="/img/logo.png" style="max-height:88px;max-width:90px;")
				.logodesc.fleft.white
					a.clear-a#logodesc-1(href="javascript:void(0)") Suzhou Hitu Outdoor Co ., Ltd 
					a.fright.clear-a#logodesc-2(href="javascript:void(0)") 苏州海途户外用品有限公司
			#header-right.fright.white(style="width:42%;")
					form.search.fright#search
						input.search-text.fleft.white(type='text' name='search' value='Search Product...')
						a.search-submit.fa.fa-search.fright(onClick="onSearch")
					a.clear-a.fright#email(href="mailto:dingjinchengyx@gmail.com") dingjinchengyx@gmail.com
					i.fright.fa.fa-email.fa-interval(style="margin-top:29px;")
	.container
		.menu.menucolor
			ul.menubar.white
				li.fleft.menuitem
					i.fa.fa-home.fa-interval
					a.clear-a(href="javascript:void(0)") HOME
				li.fleft.menuitem
					i.fa.fa-products.fa-interval
					a.clear-a(href="javascript:void(0)") PRODUCTS
				li.fleft.menuitem
					i.fa.fa-info.fa-interval
					a.clear-a(href="javascript:void(0)") NEWS
				li.fleft.menuitem
					i.fa.fa-phone.fa-interval
					a.clear-a(href="javascript:void(0)") CONTACT US
				li.fleft.menuitem
					i.fa.fa-user.fa-interval
					a.clear-a(href="javascript:void(0)") ABOUT US
		.banner
			ul.banner-img
				li.fleft.col25
					img.imgwapper(src="/img/s1.jpg")
				li.fleft.col25
					img.imgwapper(src="/img/s2.jpg")
				li.fleft.col25
					img.imgwapper(src="/img/s3.jpg")
				li.fleft.col25
					img.imgwapper(src="img/s4.jpg")
	
